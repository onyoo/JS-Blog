<!DOCTYPE html>
<html>
<head>
  <title>JSBlog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="new_post_form"></div>
  <% if !current_user %>
    <%= link_to 'Log-in', login_path %>
  <% end %>
  <br>

  <%= yield %>

</body>
</html>
